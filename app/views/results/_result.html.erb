<div class="result">
  <div class="row">
    <div class="col-8">
      <%= result.slug.titleize %>
    </div>
    <div class="col-4">
      <img src="https://www.nissanlatam.com/Catalogos/versa2020/peru/images/color/6.jpg" alt="example-image" style="width: 100%;">
    </div>
  </div>
  <%= turbo_frame_tag result.uuid do %>
    <%= link_to 'More...', result_path(result.searchable_id, q: params[:q]) %>
  <% end %>
</div>

